{
  "manifest_version": 2,
  "name": "Video Indexer",
  "version": "0.1",
  "content_scripts":[
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.3.1.js"]
    }
  ],
  "browser_action":
  {
  	 "default_popup" : "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://localhost/" 
  ]

}